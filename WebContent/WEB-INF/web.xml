<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>SpringTest</display-name>
  <servlet>
    <servlet-name>PatientControllerDispatcherServlet</servlet-name>
    <servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
    <init-param>
      <param-name>contextConfigLocation</param-name>
      <param-value>/WEB-INF/PatientController-servlet.xml</param-value>
    </init-param>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>PatientControllerDispatcherServlet</servlet-name>
    <url-pattern>/</url-pattern>
     <url-pattern>/welcome.jsp</url-pattern>
        <url-pattern>/patientslist.jsp</url-pattern>
        <url-pattern>/index.jsp</url-pattern>
        <url-pattern>/fine.jsp</url-pattern>
        <url-pattern>/addnewpatient.jsp</url-pattern>
        <url-pattern>/getpatientdetails.jsp</url-pattern>
        <url-pattern>/newspringpatient.jsp</url-pattern>
        <url-pattern>/addspringpatient.jsp</url-pattern>
  </servlet-mapping>
</web-app>
